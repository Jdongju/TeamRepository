<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.media.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="interphoneRoot" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="team1.Homvis.interphone.InterPhoneController">
   <children>
      <StackPane maxHeight="-Infinity" prefHeight="480.0" prefWidth="800.0">
         <children>
            <ImageView fitHeight="480.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true" />
            <BorderPane maxHeight="450.0">
               <right>
                  <VBox alignment="BOTTOM_CENTER" BorderPane.alignment="BOTTOM_CENTER">
                     <children>
                        <ImageView fx:id="locationBtn" fitHeight="30.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true" />
                        <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="119.0" text="공동/현관" />
                        <ImageView fx:id="phoneBtn" fitHeight="30.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                        </ImageView>
                        <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="119.0" text="통화" />
                        <ImageView fx:id="openBtn" fitHeight="30.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                        </ImageView>
                        <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="문열림">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </Button>
                        <ImageView fx:id="closeBtn" fitHeight="30.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                           <VBox.margin>
                              <Insets top="30.0" />
                           </VBox.margin>
                        </ImageView>
                        <Button mnemonicParsing="false" prefHeight="20.0" prefWidth="120.0" text="나가기">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </right>
               <center>
                  <StackPane maxHeight="-Infinity" maxWidth="-Infinity" style="-fx-background-color: black;" BorderPane.alignment="CENTER">
                     <children>
                        <MediaView fitHeight="380.0" fitWidth="650.0" />
                     </children>
                  </StackPane>
               </center>
               <top>
                  <AnchorPane maxHeight="-Infinity" prefHeight="60.0" BorderPane.alignment="CENTER">
                     <children>
                        <Label fx:id="locationlb" alignment="TOP_CENTER" layoutX="270.0" layoutY="14.0" prefHeight="20.0" prefWidth="130.0" text="공동 현관">
                           <font>
                              <Font size="30.0" />
                           </font>
                           <padding>
                              <Insets top="10.0" />
                           </padding>
                        </Label>
                     </children>
                  </AnchorPane>
               </top>
            </BorderPane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
