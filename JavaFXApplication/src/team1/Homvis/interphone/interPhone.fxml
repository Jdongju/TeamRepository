<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.media.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="450.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true" />
      <BorderPane>
         <right>
            <VBox alignment="BOTTOM_CENTER" BorderPane.alignment="BOTTOM_CENTER">
               <children>
                  <Button fx:id="phoneBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="119.0" text="통화" />
                  <Button fx:id="openBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="문열림">
                     <VBox.margin>
                        <Insets top="40.0" />
                     </VBox.margin></Button>
                  <Button fx:id="closeBtn" mnemonicParsing="false" prefHeight="20.0" prefWidth="120.0" text="나가기">
                     <VBox.margin>
                        <Insets bottom="30.0" top="50.0" />
                     </VBox.margin></Button>
               </children></VBox>
         </right>
         <top>
            <Label alignment="TOP_CENTER" text="공동 현관" BorderPane.alignment="CENTER">
               <font>
                  <Font size="30.0" />
               </font>
               <padding>
                  <Insets top="10.0" />
               </padding>
            </Label>
         </top>
         <center>
            <StackPane maxHeight="-Infinity" maxWidth="-Infinity" style="-fx-background-color: black;" BorderPane.alignment="CENTER">
               <children>
                  <MediaView fitHeight="370.0" fitWidth="650.0" />
               </children>
            </StackPane>
         </center>
      </BorderPane>
   </children>
</StackPane>
